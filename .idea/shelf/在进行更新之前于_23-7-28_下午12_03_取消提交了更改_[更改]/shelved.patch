+int NFGamePlayer::CreateInit()
+{
+    m_playerId = 0;
+    m_proxyId = 0;
+    m_logicId = 0;
+    m_gameId = 0;
+    m_roomId = 0;
+    m_deskId = INVALID_ID;
+    m_chairId = INVALID_ID;
+    m_isRobot = false;
+    m_online = false;
+    m_clearFlag = false;
+    m_offlineTime = 0;
+    m_lastMsgTime = 0;
+    m_reservedTime = 0;
+    m_reservedDeskId = INVALID_ID;
